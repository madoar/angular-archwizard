    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-safari-launcher": "^1.0.0",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.12.0",
    "protractor": "~5.4.0",
    "rxjs": "~6.5.4",
    "scss-bundle": "^2.5.1",
    "ts-node": "~8.3.0",
